algoritmo "dolarRealEmLoop"
var
     c,q:inteiro
     r,d:real
inicio
      c <- 1
      q <- 1
      escreval("quantas vezes vc quer converter ? ")
      leia(q)
   enquanto (c <= q) faca
      escreva("qual valor vc quer saber em Dollar? ")
      leia(d)
      r <- d*5
      c <- c + 1
     escreval("o valor em R$ ",r)

   fimenquanto
   
fimalgoritmo