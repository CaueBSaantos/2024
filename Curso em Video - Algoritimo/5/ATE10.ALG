algoritmo "ate10"
var
    contador,valor,salto:inteiro
inicio
       contador <- 0
       escreval("quer contar até quanto ? ")
       leia(valor)
       escreval("quer ir de quanto em quanto ?")
       leia(salto)
       enquanto (contador <= valor)faca
       escreval(contador)
       contador <- contador  + salto

       fimenquanto

fimalgoritmo